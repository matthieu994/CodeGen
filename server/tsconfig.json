{
  "compilerOptions": {
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "outDir": "build",
    "removeComments": true,
    "sourceMap": true,
    "types": [
      "node"
    ],
    "target": "es6"
    // "strict": true,
  },
  "exclude": [
    "build",
    "node_modules",
  ]
}